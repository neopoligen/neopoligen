[!- import "includes/theme.neoj" as theme -!]
<section
    [@- theme.section_classes(section, "html-section") -@] 
    [@- section.attr_string -@]>

<h3 class="pad-top-1">Output</h3>
<div class="pad-top-1">
[! autoescape false !]
[@ section.text @]
[! endautoescape !]
</div>

<h3>HTML</h3>
<div class="pad-top-1 code-button-wrapper">
<pre class="code-pre"><code>[@ highlight_code(section.text, "html") @]</code></pre>
</div>
