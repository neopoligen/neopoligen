[!- extends "wrappers/one-column.neoj" -!]
[!- include "includes/config.neoj" -!]
[!- import "includes/theme.neoj" as theme -!]

[! block body !]
[! for section in site.pages[page_id].ast !]
[! if 
  section.type != "endnote" and
  section.type != "footnote" and
  section.type != "metadata" and
  section.type != "ref" 
  !]
[@ theme.output_section(site, page_id, section) @]
[! endif !]
[! endfor !]

[! endblock !]

