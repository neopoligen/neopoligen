[!- import "includes/theme.neoj" as theme -!]

[!- for section in site.sections_for_page(page_id) -!]
[!- if 
  section.type == "forward"  or
  section.type == "tldr" 
-!]
[@- theme.output_section(site, page_id, section) -@]
[!- endif -!]
[!- endfor -!]

[!- for section in site.sections_for_page(page_id) -!]
[!- if 
  section.type != "afterword" and
  section.type != "blurb" and
  section.type != "endnote" and
  section.type != "footnote" and
  section.type != "forward" and
  section.type != "metadata" and
  section.type != "ref" and
  section.type != "tldr" and
  section.type != "title" 
  -!]
[@- theme.output_section(site, page_id, section) -@]
[!- endif -!]
[!- endfor -!]
